Host *
# PasswordAuthentication no
  ChallengeResponseAuthentication no
  HashKnownHosts yes
  VisualHostKey yes
  IdentityFile ~/.ssh/keys/id_rsa

Host *sr.ht
  IdentityFile ~/.ssh/keys/srht_id
  PreferredAuthentications publickey

Host tildeteam
  User gerd
  Hostname tilde.team
  IdentityFile ~/.ssh/keys/tilde_ecdsa
  PreferredAuthentications publickey

