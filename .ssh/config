Host *
  PasswordAuthentication no
  ChallengeResponseAuthentication no
  HashKnownHosts yes
  VisualHostKey yes
  IdentityFile ~/.ssh/keys/id_rsa

Host seedpi
  HostName seedpi.local
  IdentityFile ~/.ssh/keys/seedpi_rsa
  User pi

Host dootnet-seedpi
  HostName s.g.doot
  IdentityFile ~/.ssh/keys/seedpi_rsa
  User pi

Host github.com
  User git
  IdentityFile ~/.ssh/keys/github_rsa
